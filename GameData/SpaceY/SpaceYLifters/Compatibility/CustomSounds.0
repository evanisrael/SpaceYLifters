@PART[syl-eng-250-M1]:NEEDS[SpaceY/SpaceYLifters]:FOR[SpaceYLifters]
{
	@EFFECTS
	{
		@running_closed
		{
			@AUDIO
			{
				@clip = SpaceY/SpaceYLifters/Sounds/sls_loop_high // sound_rocket_spurts
			}
		}
	}
}

